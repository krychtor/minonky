{
  "redirects": [
    {
      "source": "/old-path",
      "destination": "/new-path",
      "statusCode": 302
    },
    {
      "source": "/(.*)",
      "destination": "https://minonky.eu/orgpad",
      "statusCode": 302,
      "has": [
        {
          "type": "host",
          "value": "orgpad.minonky.eu"
        }
      ]
    }
  ]
}
